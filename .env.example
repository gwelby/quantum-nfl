# Example environment variables - DO NOT ADD REAL VALUES HERE
# Copy this file to .env and add your values

# NFL Data API Configuration
NFL_API_KEY=your_api_key_here
NFL_API_BASE_URL=https://api.nfl.com/v1

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=quantum_nfl
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Redis Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Web Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
DEBUG=False

# Security Configuration
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here
ALLOWED_ORIGINS=http://localhost:3000,https://your-production-domain.com

# Monitoring Configuration
SENTRY_DSN=your_sentry_dsn
GRAFANA_API_KEY=your_grafana_key

# Feature Flags
ENABLE_QUANTUM_PREDICTIONS=True
ENABLE_HISTORICAL_ANALYSIS=True
ENABLE_REAL_TIME_UPDATES=False
